#include <iostream>
#include "windows.h"
using namespace std;
int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int A, B, C;
    cout << "Введіть число А\n";
    cin >> A;
    cout << "Введіть число B\n";
    cin >> B;
    cout << "Введіть число C\n";
    cin >> C;
    cout << "Чи є істинним висловлювання:Справедлива подвійна нерівність A < B < C ? \n"; 
    A < B && B < C ? cout << "Висловлювання істинне" : cout << "Висловлювання хибне";
}
